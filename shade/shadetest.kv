<Movable>:
	size_hint: None, None
	canvas:
		Color:
			rgba: [0.3,0.3,0.3,0.7]
		Rectangle:
			size: self.size
			pos: 0,0
			
			
<Panel>:
	size_hint: None, None
	canvas:
		Color:
			rgba: [1]*4
		Rectangle:
			size: self.size
			pos: self.pos
			
<Array>:
	size_hint: None, None
	canvas:
		Color:
			rgba: [0.7] * 4
		Rectangle:
			size: self.size
			pos: self.pos
			
<VoltageReading>:
	size_hint: None, None
	pos_hint: {'x': .4 ,'y': .4}
	size: 50,50
	font_size: 40